<body>
	
	<div class="container-fluid text-center" >
	<div class="row content">
		<div class="col-sm-3 sidenav"></div>
		<div class="col-sm-7 text-right" >
			<h1>קטלוג</h1>
		</div>
			<div class="col-sm-8 text-left">
			<ul style="margin-top: 100px; margin-left: 100px;">
					<% flowers.forEach(function(flower) { %>
						<div class="col-sm-4" >
							<div class="card" style="margin-left:10px; margin-top: 7px">
								<div class="card-front" >
										<a target="_blank" href="images\<%=flower.imag %>">
												<img src="images\<%=flower.imag %>" alt="flowers" width="100%" height="100%"></background-image:></a>
											</div>
								<div class="card-back" style="font-family:AR CENA; font-size:18px" >
									<h2 >name: <%=flower.name %></h2>
										 <br> color: <%=flower.color %>
										 <br> price: <%=flower.price %></div>
							</div>
						</div>
					<% }); %>
		</ul>
	</div>	
	</div>
	</div>
</body>
  